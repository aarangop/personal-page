generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model PodcastFeeds {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  slug String @unique
  rssFeed String @unique 
}

model BlogPost {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  subtitle String
  fileUrl String
  imageUrl String
}